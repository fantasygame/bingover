- @users.each do |user|
  %table.table.table.striped.game
    %tr
      %th{ colspan: 5}= "Game of #{user.email}"
    - user.user_frazes.order(:created_at).each_slice(5) do |slice|
      %tr
        - slice.each do |user_fraze|
          %td
            - if user_fraze.checked?
              %strong X
            = user_fraze.fraze.body


