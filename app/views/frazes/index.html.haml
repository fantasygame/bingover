%h1 Listing frazes

= link_to 'New Fraze', new_fraze_path, class: 'btn btn-success'

%table
  %thead
    %tr
      %th Body
      %th
      %th
      %th

  %tbody
    - @frazes.each do |fraze|
      %tr
        %td= fraze.body
        %td= link_to 'Show', fraze
        %td= link_to 'Edit', edit_fraze_path(fraze)
        %td= link_to 'Destroy', fraze, :method => :delete, :data => { :confirm => 'Are you sure?' }

%br
